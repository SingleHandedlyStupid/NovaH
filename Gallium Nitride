   end,
})


--//goddamn finally, ghost stuff.
local SolveKey = Ghost:CreateButton({
   Name = "Solve Keypad",
   Callback = function()
   local P=game.workspace["Keypad"]
   if P then
    local code=solve()
    for i=1,#code do
        fireclickdetector(P["Buttons"][string.sub(code,i,i)].ClickDetector)
        task.wait(.3)
    end
    fireclickdetector(P["Buttons"]["Enter"].ClickDetector)
   end
   end,
})

local SpyS = Misc:CreateButton({
   Name = "Load RemoteSpy",
   Callback = function()
   loadstring(game:HttpGetAsync("https://github.com/richie0866/remote-spy/releases/latest/download/RemoteSpy.lua"))()
   end,
})

local bob = Misc:CreateButton({
   Name = "Replica auto-bob",
   Callback = function()
   game.ReplicatedStorage.bob:FireServer()
   end,
})

local GlovelDig350 = Misc:CreateButton({
   Name = "Glovel Dig Mastery | Cannot be turned off till end",
   Callback = function()
   Rayfield:Notify({
   Title = "GaN",
   Content = "Please, do not move while digging, this may take a while.",
   Duration = 6.5,
   Image = "circle-check",
})
   local remate = game:GetService("ReplicatedStorage").GlovelCancel
   local remote = game:GetService("ReplicatedStorage").GlovelFunc
		for i = 1, 350 do

			-- yield happens here, not in your main thread
			remote:InvokeServer()
			remate:FireServer()

			task.wait() -- 1 frame, prevents scheduler abuse
		end
   Rayfield:Notify({
   Title = "Glovel",
   Content = "350 Complete, Check Mastery Page.",
   Duration = 6.5,
   Image = "circle-check",
})
   end,
})

local CollectSlapples = Blatant:CreateButton({
   Name = "Collect All Slapples",
   Callback = function()
   local kids=workspace.Arena.island5.Slapples:GetChildren()
   Game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored=true
   for _,k in pairs(kids) do
    if k.Glove.Transparency==0 then
        Game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = k.Glove.CFrame
        Game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored=false
            task.wait(.1)
            Game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored=true
    end
   end
local P=Vector3.new(-407.5, 50.2, -23.6)
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(P)
   task.wait(1)
   Game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Running)
   Game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored=false
   end,
})

local GhostExploit = Ghost:CreateButton({
   Name = "Ghost Invis Exploit",
   Callback = function()
   if game:GetService("Players").LocalPlayer.leaderstats.Glove.Value=="Ghost" then
    local remote = game:GetService("ReplicatedStorage").Ghostinvisibilityactivated
    remote:FireServer()
   else
       Rayfield:Notify({
        Title = "GaN",
        Content = "Equipping Ghost!",
        Duration = 3.5,
        Image = "circle-check",
    })
    fireclickdetector(workspace.Lobby.Ghost.ClickDetector)
    local oldG=game:GetService("Players").LocalPlayer.leaderstats.Glove.Value
        local remote = game:GetService("ReplicatedStorage").Ghostinvisibilityactivated
    remote:FireServer()
    fireclickdetector(workspace.Lobby[oldG].ClickDetector)
   end
   end,
})

local ToggleNoCatch = BetaTab:CreateToggle({
    Name = "Leave on Expose",
    CurrentValue = false,
    Flag = "LoE",
    Callback = function(Value)
        no_expose = Value
    end,
})


for _,k in pairs(game.Players:GetPlayers()) do
    k.Chatted:Connect(function(msg)
    if no_expose==true then
            if string.find(msg:lower(),"cheater") or string.find(msg:lower(),"invi") or string.find(msg:lower(),"exploit") or string.find(msg:lower(),"hacker") or string.find(msg:lower(),"rec") then
               Rayfield:Notify({
        Title = "GaN",
        Content = "Detection went off",
        Duration = 3.5,
        Image = "circle-check",
    })
    task.wait(.3)
        Game.Players.LocalPlayer:Kick("DETECTED")
    end
    end
end)
end
game.Players.PlayerAdded:Connect(function(player)
player.Chatted:Connect(function(msg)
    if no_expose==true then
            if string.find(msg:lower(),"cheater") or string.find(msg:lower(),"invi") or string.find(msg:lower(),"exploit") or string.find(msg:lower(),"hacker") or string.find(msg:lower(),"rec") then
               Rayfield:Notify({
        Title = "GaN",
        Content = "Detection went off",
        Duration = 3.5,
        Image = "circle-check",
    })
    Rayfield:Destroy()
    task.wait(.3)
        Game.Players.LocalPlayer:Kick("DETECTED")
    end
    end
end)
end)
