local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local vUrl="https://raw.githubusercontent.com/SingleHandedlyStupid/NovaH/refs/heads/main/VERSIONFILE"

local barriers = {
	"dedBarrier",
	"AntiDefaultArena",
	"DEATHBARRIER",
	"DEATHBARRIER2",
	"Antidream",
	"ArenaBarrier"
}

for _, name in ipairs(barriers) do
	local obj = workspace:FindFirstChild(name)
	if obj then
		obj:Destroy()
	end
end


local VOIDP = workspace:FindFirstChild("VoitAnti")
if VOIDP then VOIDP:Destroy() end

local ppa=Instance.new("Part")
ppa.Name="VoidAnti"
ppa.Parent=workspace

ppa.Size = Vector3.new(2048, 1, 2048)
ppa.Position=Vector3.new(14.9, -9, 41.5)
ppa.Transparency=0.6
ppa.Anchored=true

local dropdowntravel={
    ["Castle Island"] = CFrame.new(Vector3.new(261.8, 33.6, 198.2)),
    ["Main Island"] = CFrame.new(Vector3.new(-407.5, 50.2, -23.6)),
    ["Slapple Island"] = CFrame.new(Vector3.new(-407.5, 50.2, -23.6)),


}

local no_expose=false

local lastJump = 0
local JUMP_COOLDOWN = 0.2

-- fetch version file
local UrlContents = game:HttpGet(vUrl)

-- split at first semicolon
local firstSemi = UrlContents:find(";")
local versionStr, changelog

if firstSemi then
    versionStr = UrlContents:sub(1, firstSemi - 1)
    changelog = UrlContents:sub(firstSemi + 1)
else
    versionStr = UrlContents
    changelog = "No changelog available"
end

-- convert to number safely
versionStr = versionStr:gsub("%s+", "") -- remove whitespace
local versionNum = tonumber(versionStr)

local VersionCurrent = 2.6

print("Current script version:", VersionCurrent)
print("Latest version:", versionNum)
print("Changelog:", changelog)

function solve()
    local AD=(#Game.Players:GetPlayers() * 25) + 1100 - 7
    print("code is "..tostring(AD))
    return tostring(AD)
end

print("version "..version or "nil")

print("changelog? "..changelog or "nil")

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local PathfindingService = game:GetService("PathfindingService")

local localPlayer = Players.LocalPlayer

local Window = Rayfield:CreateWindow({
   Name = "GaN",
   Icon = "atom", -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
   LoadingTitle = "Gallium Nitride",
   LoadingSubtitle = "by SEMI-COLON",
   ShowText = "Ga", -- for mobile users to unhide rayfield, change if you'd like
   Theme = "Amethyst", -- Check https://docs.sirius.menu/rayfield/configuration/themes

   ToggleUIKeybind = "K", -- The keybind to toggle the UI visibility (string like "K" or Enum.KeyCode)

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, -- Prevents Rayfield from warning when the script has a version mismatch with the interface

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "GaN"
   },

   Discord = {
      Enabled = false, -- Prompt the user to join your Discord server if their executor supports it
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },

   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided", -- Use this to tell the user how to get a key
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Hello"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

Rayfield:Notify({
   Title = "GaN",
   Content = "Loaded GaN!",
   Duration = 6.5,
   Image = "circle-check",
})
Rayfield:Notify({
   Title = "GaN",
   Content = "Logged in as "..Game.Players.LocalPlayer.Name,
   Duration = 6.5,
   Image = "user-check",
})

-- version check
if versionNum and VersionCurrent ~= versionNum then
    Rayfield:Notify({
        Title = "Gallium Nitride",
        Content = "Version mismatch ("..VersionCurrent.."), New version available "..versionNum,
        Duration = 6.5,
        Image = "history",
    })
    Rayfield:Notify({
        Title = "Gallium Nitride",
        Content = "Auto loading "..versionNum,
        Duration = 6.5,
        Image = "history",
    })
    Rayfield:Destroy()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/SingleHandedlyStupid/NovaH/refs/heads/main/Gallium%20Nitride"))()
end


